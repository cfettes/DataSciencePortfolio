--Find the distribution of star ratings to the business in the following cities:
--a. Avon
SELECT stars as rating, COUNT(*) as count
FROM business
WHERE city = 'Avon'
GROUP BY stars
ORDER BY stars ASC;
/*
+--------+-------+
| rating | count |
+--------+-------+
|    1.5 |     1 |
|    2.5 |     2 |
|    3.5 |     3 |
|    4.0 |     2 |
|    4.5 |     1 |
|    5.0 |     1 |
+--------+-------+
*/

--b. Beachwood
SELECT stars as rating, COUNT(*) as count
FROM business
WHERE city = 'Beachwood'
GROUP BY stars
ORDER BY stars ASC;
/*
+--------+-------+
| rating | count |
+--------+-------+
|    2.0 |     1 |
|    2.5 |     1 |
|    3.0 |     2 |
|    3.5 |     2 |
|    4.0 |     1 |
|    4.5 |     2 |
|    5.0 |     5 |
+--------+-------+
*/

