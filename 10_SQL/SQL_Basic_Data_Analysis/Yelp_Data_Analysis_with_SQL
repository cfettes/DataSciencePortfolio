--Are there more reviews with the word "love" or with the word "hate" in them?
SELECT COUNT(*)									
FROM review
WHERE text LIKE "%love%";	
/*
+----------+
| COUNT(*) |
+----------+
|     1780 |
+----------+
*/

SELECT COUNT(*)									
FROM review
WHERE text LIKE "%hate%";	
/*
+----------+
| COUNT(*) |
+----------+
|      232 |
+----------+
*/
--more reviews with the word love

